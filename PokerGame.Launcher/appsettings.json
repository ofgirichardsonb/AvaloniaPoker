{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ProcessManager": {
    "DotNetPath": "dotnet",
    "AutoDetectDotNetPath": true,
    "ProjectBasePath": "",
    "ConsoleProjectPath": "PokerGame.Console/PokerGame.Console.csproj",
    "ServicesProjectPath": "PokerGame.Services/PokerGame.Services.csproj"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "${APPINSIGHTS_INSTRUMENTATIONKEY}",
    "EnableAdaptiveSampling": true,
    "EnablePerformanceCounterCollectionModule": true,
    "EnableDependencyTrackingTelemetryModule": true,
    "EnableRequestTrackingTelemetryModule": true,
    "EnableHeartbeat": true
  }
}